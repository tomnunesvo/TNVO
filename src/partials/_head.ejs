<head>
  <%- include('_analytics.ejs') %>
  <meta charset="utf-8">
  <meta name="viewport" content="width=device-width, initial-scale=1">
  <meta class="name">
  </meta>
  <title>
    <%= meta.title %>
  </title>
  <meta name="description" content="<%= meta.description %>">
  <link rel="canonical" href="<%= meta.url %>">
  <meta property="og:locale" content="en_US">
  <meta property="og:type" content="website">
  <meta property="og:title" content="<%= meta.name %> <%= whatIDo %>">
  <meta property="og:description" content="<%= meta.description %>">
  <meta property="og:url" content="<%= meta.url %>">
  <meta property="og:site_name" content="<%= meta.name %> <%= whatIDo %>">
  <meta property="og:image" content="<%= meta.socialImage %>">
  <meta property="og:image:type" content="<%= meta.socialImageType %>">
  <meta property="og:image:width" content="<%= meta.socialImageWidth %>">
  <meta property="og:image:height" content="<%= meta.socialImageHeight %>">
  <meta property="og:image:secure_url" content="<%= meta.socialImage %>">
  <%#<meta name="twitter:card" content="tbd">%>
  <%#<meta name="twitter:description" content="tbd">%>
  <%#<meta name="twitter:title" content="tbd">%>
  <%#<meta name="twitter:image" content="tbd">%>
  <link href="https://fonts.googleapis.com/css?family=Nunito:400,400i,700,Lato:300,400,400i,700" rel="stylesheet">
  <%#<link href="https://fonts.googleapis.com/css?family=Alegreya|Lato:300,400,400i,600" rel="stylesheet">%>
  <%#<link href="https://fonts.googleapis.com/css?family=Fjalla+One|Noto+Sans" rel="stylesheet">%>
  <link rel="stylesheet" href="<%= relPath %>css/tnvo.min.css?cb=<%= cacheBuster %>">
  <%- include('_favicons.ejs') %>
  <style>
    .show-vr-grid {
      background: linear-gradient(rgba(0, 119, 179, 0.2) 1px, transparent 1px) 0 0 / 1.5rem 1.5rem;
      background-position: left -5px;
    }
  </style>
  <script type="application/ld+json"><%- ldJSON %></script>
</head>